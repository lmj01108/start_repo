# 컴퓨터공학부 202102395 이명진

class BankAccount: # 은행계좌 클래스
	def __init__(self, acc_name=None, acc_number=None, balance=0):
		self.account_name = acc_name
		self.account_number = acc_number
		self.balance = balance
	
	def show_balance(self):
		print(self.balance)
	
	def show_account_info(self):
		print(self.account_number, self.account_name, self.balance)
	
	def deposit(self, amount):
		self.balance += amount
	
	def withdraw(self, amount):
		self.balance -= amount
	
	def double(self):
		self.balance *=2

acc = BankAccount("Mr. Kim", 10476, 30000)
acc.withdraw(10000)
acc.show_balance()
acc.deposit(20000)

def maximum(a,b,c): # 최댓값 출력 함수
	if a>b:
		if a>c:
			return a
		else:
			return c
	else:
		if b>c:
			return b
		else:
			return c
			
a,b,c = input().split() #스페이스기준으로 문자열을 찢어서 각 요소의 리스트를 만들 수 있음
a,b,c = int(a), int(b), int(c)

print("입력한 숫자: ", a, b, c)		

class Node: # node 클래스
	def __init__(self, key=None, value=None, left=None, right=None):
		self.item = (key, value)
		self.left, self.right = left, right
		
	def key(self):
		return self.item[0]

	def value(self):
		return self.item[1]

	def set_value(self, value):
		self.item[1] = value
